n    pageBoolean:{\n      onBeforeSend: function(val:any, oldVal:any, options:{proceed:boolean}) {\n        console.log('onBeforeSend for pageBoolean in page1', val, oldVal, options);\n      },\n      onBeforeReceive: function(val:any, oldVal:any, options:{proceed:boolean}) {\n        console.log('onBeforeReceive for pageBoolean in page1', val, oldVal, options);\n      },\n    },\n  },\n  globalStore:{\n    globalObject:{\n      deep:true, //an option of watch\n    },\n    globalCounter:{},\n  },\n  methods:{\n    resetPageObject() {\n      this.pageObject = {\n        strValue1:'strValue1 default',\n        strValue2:'strValue2 default',\n        numberValue1:10,\n        nestedObj :{\n          nestedStrVal1:'nestedStrVal1 string',\n          nestedNumberVal1:101,\n        }\n      };\n    },\n    resetGlobalObject() {\n      this.globalObject = {\n        strValue1:'strValue1 default',\n        strValue2:'strValue2 default',\n        numberValue1:10,\n        nestedObj :{\n          nestedStrVal1:'nestedStrVal1 string',\n          nestedNumberVal1:101,\n        }\n      };\n    }\n  }\n}\n);\n</script>\n\n<style>\ntd {\n  text-align: left;\n  width: 50%;\n}\n</style>"]}]}